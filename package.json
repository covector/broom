{
  "name": "broom",
  "version": "1.0.0",
  "description": "A Chrome extension for managing tab groups",
  "author": "covector",
  "license": "MIT",
  "scripts": {
    "start": "webpack --config webpack/webpack.dev.js --watch",
    "build": "webpack --config webpack/webpack.prod.js",
    "popup-css": "sass --no-source-map --watch style/popup.scss:dist/popup.css",
    "options-css": "sass --no-source-map --watch style/options.scss:dist/options.css"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.145",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "sass": "^1.48.0",
    "ts-loader": "^9.2.3",
    "typescript": "^4.3.5",
    "webpack": "^5.42.0",
    "webpack-cli": "^4.7.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
